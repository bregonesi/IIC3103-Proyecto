<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Bruto</th>
      <th>Iva</th>
      <th>Total</th>
      <th>Proveedor</th>
      <th>Cliente</th>
      <th>Oc</th>
      <th>Estado</th>
      <th>Rechazo</th>
      <th>Anulacion</th>
      <th>Originator type</th>
      <th>Originator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice._id %></td>
        <td><%= invoice.bruto %></td>
        <td><%= invoice.iva %></td>
        <td><%= invoice.total %></td>
        <td><%= invoice.proveedor %></td>
        <td><%= invoice.cliente %></td>
        <td><%= invoice.oc %></td>
        <td><%= invoice.estado %></td>
        <td><%= invoice.rechazo %></td>
        <td><%= invoice.anulacion %></td>
        <td><%= invoice.originator_type %></td>
        <td><%= invoice.originator %></td>
        <td><%= link_to 'Show', invoice %></td>
        <td><%= link_to 'Edit', edit_invoice_path(invoice) %></td>
        <td><%= link_to 'Destroy', invoice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice', new_invoice_path %>
